<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.BorderPane?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<BorderPane fx:id="content_pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="idol.tofu.client.ReservationController">
    <top>
        <VBox alignment="CENTER_LEFT">
            <padding>
                <Insets topRightBottomLeft="5"/>
            </padding>
            <children>
                <Label fx:id="top_heading" text="예약"/>
            </children>
        </VBox>
    </top>
    <bottom>
        <HBox alignment="CENTER_RIGHT" spacing="10">
            <padding>
                <Insets topRightBottomLeft="10"/>
            </padding>
            <children>
                <Button fx:id="prevBtn" onAction="#handlePrevBtn" text="이전" />
                <Button fx:id="nextBtn" onAction="#handleNextBtn" text="다음" />
            </children>
        </HBox>
    </bottom>
</BorderPane>
